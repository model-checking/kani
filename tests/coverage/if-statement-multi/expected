  13|    0| ```    println!("Hello, world!");'''
  14|    0| ```}'''
  15|     | 
  16|    2| fn test_cov(val: u32) -> bool {
  17|    2|     if val < 3 || ```val == 42''' { true } else { ```false''' }
  18|     | }
  19|     | 
  20|     | #[cfg_attr(kani, kani::proof)]
