   4|    2| fn find_positive(nums: &[i32]) -> Option<i32> {
   5|    2|     for &num in nums {
   6|     |         if num > 0 {
   7|    2|             return Some(num);
   8|    2|         }
   9|     |     }
  10|     |     // `None` is unreachable because there is at least one positive number.
  11|    0|     None
  12|     | }
